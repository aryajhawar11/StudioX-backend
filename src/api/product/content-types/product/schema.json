{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "description": {
      "type": "blocks"
    },
    "use_case_purpose": {
      "type": "blocks"
    },
    "key_benefits": {
      "type": "blocks"
    },
    "indicative_cost": {
      "type": "string"
    },
    "installation_time": {
      "type": "string"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vehicle.vehicle",
      "inversedBy": "products"
    }
  }
}
